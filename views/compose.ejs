<%- include('partials/header') -%>

<h1>Compose</h1>
<form action="/compose" method="POST">
  <div class="form-group">
    <label for="">Title</label>
    <input class="form-control" type="textarea" name="newEntry" />
    <label for="">Post</label>
    <textarea
      class="form-control"
      name="entryBody"
      cols="30"
      rows="5"
    ></textarea>
  </div>
  <button class="btn" type="submit" name="button">Publish</button>
</form>

<%- include('partials/footer') -%>
